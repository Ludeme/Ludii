        // Rook
        (piece "Rook" Each ("SlideMove" Orthogonal "CanPromote"))

        // Vice General
        (piece "ViceGeneral" Each ("StepMove" (directions {Forward Leftward Rightward BL BR FL FR})))

        // Queen
        (piece "Queen" Each ("SlideMove"))

        // Lion
