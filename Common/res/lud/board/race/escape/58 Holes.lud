(option "Variant" <Variant> args:{ <hand> <region> <pieceMove> <map> <rules> }
    {
    (item "Carnarvon and Carter" 
        
        <(hand Each)>
        
        <(regions "Replay" {14 24 43 53})> 
        
        <
        (if (= "MoveTo" 58)
            "RemovePiece"
            (if ("IsNotOffBoard" "MoveTo") 	 			
                (if (is Empty "MoveTo")
                    (move 
                        (from) 
                        (to "MoveTo")
                        (then
                            (and
                                ("ReplayInMovingOn" (sites "Replay"))
                                ("Teleportation")
                            )
                        )
                    )
                )
            )
        )
        > 
        
        <(map {(pair 19 5) (pair 9 7) (pair 48 34) (pair 38 36) })>
        
        <
        (define "Teleportation"
            (if (!= (last To) (mapEntry (last To)))
                (if (is Empty (mapEntry (last To)))
                    (move (from (last To)) (to (mapEntry (last To))))
                )
            )		
        )
        
        (rules 
            (start {
                (place "Marker1" (handSite P1) count:5)
                (place "Marker2" (handSite P2) count:5)
            })
            
            (play ("RollMove" (or (forEach Piece) (forEach Piece container:(mover)))))
            (end ("EscapeWin"))
        )
        >
        
        "Rules from Carnarvon and Carter."
    )*
    
    (item "Carter Marked" 
        
        <(hand Each)>
        
        <(regions "Replay" {0 5 7 9 14 19 24 58 53  48 43 38 36 34 29})> 
        
        <
        (if (= "MoveTo" 58)
            "RemovePiece"
            (if ("IsNotOffBoard" "MoveTo") 	 			
                (if (is Empty "MoveTo")
                    (move 
                        (from) 
                        (to "MoveTo")
                        (then
                            ("ReplayInMovingOn" (sites "Replay"))
                        )
                    )
                )
            )
        )
        > 
        
        <(map {(pair 19 5) (pair 9 7) (pair 48 34) (pair 38 36) })>
        
        <
        (rules 
            (start {
                (place "Marker1" (handSite P1) count:5)
                (place "Marker2" (handSite P2) count:5)
            })
            
            (play ("RollMove" (or (forEach Piece) (forEach Piece container:(mover)))))
            (end ("EscapeWin"))
        )
        >
        
        "Rules from Carter with marked holes."
    )
    
    (item "Carter Unmarked" 
        
        <(hand Each)>
        
        <> 
        
        <
        (if (= "MoveTo" 58)
            "RemovePiece"
            (if ("IsNotOffBoard" "MoveTo") 	 			
                (if (is Empty "MoveTo")
                    (move 
                        (from) 
                        (to "MoveTo")
                    )
                )
            )
        )
        > 
        
        <(map {(pair 19 5) (pair 9 7) (pair 48 34) (pair 38 36) })>
        
        <
        (rules 
            (start {
                (place "Marker1" (handSite P1) count:5)
                (place "Marker2" (handSite P2) count:5)
            })
            
            (play ("RollMove" (or (forEach Piece) (forEach Piece container:(mover)))))
            (end ("EscapeWin"))
        )
        >
        
        "Rules from Carter with no marked holes."
    )
    
    (item "Carter Crossover 1" 
        
        <(hand Each)>
        
        <> 
        
        <
        (if (= "ProgressOnTrack" 58)
            "RemovePiece"
            (if ("IsNotOffBoard" "ProgressOnTrack") 	 			
                (if (not ("IsFriendAt" "ProgressOnTrack"))
                    (move 
                        (from) 
                        (to "ProgressOnTrack"
                            (apply
                                (if ("IsEnemyAt" (to)) (move (from (to)) (to (handSite (who at:(to))))))
                            )
                        )
                        (then
                            (if (!= (last To) (mapEntry (last To))) (moveAgain))
                        )
                    )
                )
            )
        )
        > 
        
        <(map {(pair 5 36) (pair 34 7) (pair 14 19) (pair 19 24) (pair 43 48) (pair 48 53) })>
        
        <
        
        (define "ProgressOnTrack" 
            (if (or (= (from) 59) (<= (from) 28))
                ("NextSiteOnTrack" (count Pips) "Track1")
                ("NextSiteOnTrack" (count Pips) "Track2")
            )
        )
        
        (rules 
            (start {
                (place "Marker1" (handSite P1) count:5)
                (place "Marker2" (handSite P2) count:5)
            })
            
            (play 
                (if ("SameTurn")
                    (or
                        (move Pass)
                        (move 
                            (from (last To))
                            (to (mapEntry (last To))
                                (apply
                                    (if ("IsEnemyAt" (to)) (move (from (to)) (to (handSite (who at:(to))))))
                                )		
                            )
                            (then
                                (if (!= (last To) (mapEntry (last To))) (moveAgain))
                            )
                        )
                    )
                    ("RollMove" (or (forEach Piece) (forEach Piece container:(mover))))
                )
            )
            (end ("EscapeWin"))
        )
        >
        
        "Rules from Carter Crossover 1."
    )
    
    (item "Carter Crossover 2" 
        
        <(hand Each)>
        
        <> 
        
        <
        (if (= "ProgressOnTrack" 58)
            "RemovePiece"
            (if ("IsNotOffBoard" "ProgressOnTrack") 	 			
