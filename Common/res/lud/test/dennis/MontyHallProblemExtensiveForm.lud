	        					(set Hidden (sites "Subgraph_0") to:All)
	        					(set NextPlayer (player 1))
	        				}))
	        			)
	        			(move Select (from 1)
	        				(then (and {
	        					(fromTo (from 8) (to 19))
	        					(remove (sites Occupied by:P1))
	        					(add (piece 1) (to (sites "InformationSet_19_1")))
	        					(set Hidden (sites "Subgraph_0") to:All)
	        					(set NextPlayer (player 1))
	        				}))
	        			)
					}
				)
			(if (= (where "Marker" Neutral) 9)
				// Vertex 9: door getting opened leads to vertex 20
				(random
					{1}
					{
						(move Select (from 0)
	        				(then (and {
