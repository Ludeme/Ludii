        (regions P2 
            (sites Top)
        )
        
        (map { 
            (pair 1 23)
            
            (pair 2 11)
            }
        )
        }
    )
    
    (rules 
        (start { 
            (place Stack "Disc1" 23 count:14)
            
            (place Stack "Disc2" 11 count:14)
            }
        )
        
        (play 
            (do 
                (if 
                    (not 
                        (is Prev Mover)
                    )
                    
                    (roll)
                )
                next:
                (forEach Die if:
                    (!= 
                        (pips)
                    0)
                    
                    (forEach Piece top:True 
                        (then 
                            (if 
                                (not 
                                    (all DiceUsed)
                                )
                                
                                (moveAgain)
                            )
                        )
                    )
                )
            )
        )
        
        (end 
            (if 
                (no Pieces Mover)
                
                (result Mover Win)
