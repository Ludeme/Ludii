(item "Los Romanos Rencontrat1" <
 
    (players 2)
    
    (equipment { 
        (board 
            (merge { 
                (rectangle 1 6)
                
                (shift 7 0 
                    (rectangle 1 6)
                )
                
                (shift 0 6 
                    (rectangle 1 6)
                )
                
                (shift 7 6 
                    (rectangle 1 6)
