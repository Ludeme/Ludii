(game "Hamiltonian Maze"
    (players 1)
    (equipment {
        (board <Board:type> use:Vertex)
    })

    (rules
        (play
            (move
                Add
                (to
                    Edge
                    (sites Empty Edge)
                )
            )
        )

        (end {
            (if
                (is Path Edge Mover length:(exact (count Sites "Board")) closed:True)
                (result Mover Win)
            )
            (if
                (is Path Edge Mover length:(max (count Moves)) closed:True)
                (result Mover Loss)
            )
        })
    )
)

